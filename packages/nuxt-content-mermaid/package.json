    "@types/mermaid": "^9.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "^3.3.1",
    "prettier": "^2.8.4",
    "typescript": "^5.0.2",
    "vue-tsc": "^1.2.0"
