{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "prepare": {
      "outputs": [".nuxt"]
    },
    "@d0rich/nuxt-design-system#prepare": {
      "dependsOn": ["@d0rich/esprit-design#build"]
    },
    "@d0rich/esprit-design#build": {
      "outputs": ["dist"]
    },
    "generate": {
      "dependsOn": ["^generate"],
      "outputs": ["dist"]
    },
    "typecheck": {},
    "test": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "d0rich.me#dev": {},
    "d0xigen.d0rich.me#dev": {},
    "design.d0rich.me#dev": {},
    "nuxt-content-mermaid-playground#dev": {}
  }
}
