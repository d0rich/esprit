{
  "name": "@d0rich/dog",
  "description": "A CLI tool to generate favicons and app icons for your web app",
  "version": "1.0.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikolai Dorofeev",
    "email": "dorich2000@gmail.com",
    "url": "https://d0rich.me/"
  },
  "keywords": [
    "cli",
    "favicon",
    "icon",
    "web",
    "app",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/d0rich/esprit",
    "directory": "utils/dog"
  },
  "bin": {
    "dog": "./dist/cli.js"
  },
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsc",
    "dev:web": "tsx watch ./src/runWeb.ts",
    "dev:cli": "tsc -w ./src/cli.ts",
    "run:web": "node ./dist/runWeb.js"
  },
  "dependencies": {
    "@backroad/backroad": "^1.5.0",
    "@types/node": "^22.10.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^12.1.0",
    "node-html-to-image": "^5.0.0",
    "png-to-ico": "^2.1.8",
    "unstorage": "^1.13.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
